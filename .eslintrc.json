{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"env": {
		"browser": true
	},
	"plugins": [ "vue" ],
	"rules": {
		"max-len": [ 2, { "code": 250, "tabWidth": 2, "ignoreUrls": true }],
		"eol-last": [0],
		"quote-props": [0],
		"global-require": [0],
		"no-var": [0],
		"no-new": [0],
		"no-undef": [0],
		"no-console": [0],
		"no-plusplus": [0],
		"no-trailing-spaces": [0],
		"no-unused-vars": [0],
		"no-useless-concat": [0],
		"no-use-before-define": [0],
		"prefer-arrow-callback": [0],
		"space-before-function-paren": [0],
		"func-names": [0],
		"import/no-unresolved": [0],
        "import/extensions": [0],
        "array-bracket-spacing": [0]
	}
}